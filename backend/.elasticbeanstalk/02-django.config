container_commands:
  01_collectstatic:
    command: "source $PYTHONPATH/activate && collectstatic"
  02_migrate:
    command: "source $PYTHONPATH/activate && python manage.py migrate" 
    leader_only: true
  02_compilemessages:
    command: "source $PYTHONPATH/activate && python manage.py compilemessages"


option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: config.settings
    "PYTHONPATH": "/opt/python/current/app/config:$PYTHONPATH"
  aws:elasticbeanstalk:container:python:
    WSGIPath: config/config/wsgi.py
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"