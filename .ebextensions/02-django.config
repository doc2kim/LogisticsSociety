container_commands:
   01_makemigrations:
      command: "source $PYTHONPATH/activate && python manage.py makemigrations"
   02_migrate:
      command: "source $PYTHONPATH/activate && python manage.py migrate"
      leader_only: true
   03_react:
      command: "npm install && npm run build"
      ignoreErrors: false
   04_collectstatic: 
      command: "source $PYTHONPATH/activate && python manage.py collectstatic --noinput"
   05_createsu:
      command: "source $PYTHONPATH/activate && python manage.py createsu"
      leader_only: true

option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: config.settings
  aws:elasticbeanstalk:container:python:
    WSGIPath: config.wsgi:application
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    /static: staticfile
    
  