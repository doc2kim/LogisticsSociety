container_commands:
 01_makemigrations:
    command: "source $PYTHONPATH/activate && python manage.py makemigrations"
 02_migrate:
    command: "source $PYTHONPATH/activate && python manage.py migrate" 
 03_collectstatic:
    command: "source $PYTHONPATH/activate && python manage.py collectstatic"
 04_compilemessages:
    command: "source $PYTHONPATH/activate && python manage.py compilemessages"
  

option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: config.settings
  aws:elasticbeanstalk:container:python:
    WSGIPath: config.wsgi:application